        android:id="@+id/vFourth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/vSixth"
        app:layout_constraintTop_toBottomOf="@id/vFourth"
        android:background="#18FFFF"
        android:id="@+id/vFifth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/vSeventh"
        app:layout_constraintTop_toBottomOf="@id/vFifth"
        android:background="#69F0AE"
        android:id="@+id/vSixth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/vEighth"
        app:layout_constraintTop_toBottomOf="@id/vSixth"
        android:background="#D81B60"
        android:id="@+id/vSeventh"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/vNinth"
        app:layout_constraintTop_toBottomOf="@id/vSeventh"
        android:background="#C5D81B"
        android:id="@+id/vEighth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/vTenth"
        app:layout_constraintTop_toBottomOf="@id/vEighth"
        android:background="#20E8CA"
        android:id="@+id/vNinth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/vNinth"
        android:background="#1A237E"
        android:id="@+id/vTenth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bThird"
        app:layout_constraintStart_toEndOf="@id/vTenth"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@color/black"
        android:id="@+id/bSecond"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bFourth"
        app:layout_constraintStart_toEndOf="@id/bSecond"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#DA3232"
        android:id="@+id/bThird"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bFifth"
        app:layout_constraintStart_toEndOf="@id/bThird"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#1A237E"
        android:id="@+id/bFourth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bSixth"
        app:layout_constraintStart_toEndOf="@id/bFourth"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#006064"
        android:id="@+id/bFifth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bSeventh"
        app:layout_constraintStart_toEndOf="@id/bFifth"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#E1DA52"
        android:id="@+id/bSixth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bEighth"
        app:layout_constraintStart_toEndOf="@id/bSixth"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#EF147B"
        android:id="@+id/bSeventh"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bNinth"
        app:layout_constraintStart_toEndOf="@id/bSeventh"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#14EFE8"
        android:id="@+id/bEighth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toStartOf="@id/bTenth"
        app:layout_constraintStart_toEndOf="@id/bEighth"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#5D0064"
        android:id="@+id/bNinth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/bNinth"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#EAE7B5"
        android:id="@+id/bTenth"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>